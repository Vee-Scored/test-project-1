{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites" : [
    {
      "source" : "/(.*)",
      "destination" : "/index.html"
    }
  ],
  "buildCommand" : "npm run build",
  "outputDirectory" : "dist",
  "framework" : "vite"
}